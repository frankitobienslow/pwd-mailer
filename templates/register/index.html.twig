{% extends 'base.html.twig' %}

{% block title %}
{{parent()}} - Registrarse
{% endblock %}

{% block body %}

<div class="container d-flex w-50 justify-content-center align-items-center">
<div class="row w-50">
<div class="col-6 bg-white p-5"></div>
<h3>Registro</h3>
{{form(form)}}
</div>
{% for flash_message in app.session.flashbag.get('success') %}
    <div class="flash-notice">
        {{ flash_message }}
    </div>
{% endfor %}
</div>



{% endblock %}
